{
    "name": "slim-i18n",
    "version": "1.0.3",
    "description": "Slim i18n library + utils for react components",
    "author": "Ihor Yemets <ya.peren@yandex.ru>",
    "contributors": [
        "Maksym Tymchyk <maksym.tymchyk@gmail.com>"
    ],
    "repository": "https://github.com/IOuser/slim-i18n",
    "license": "WTFPL",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "npm run build:ts",
        "build:ts": "tsc",
        "tslint": "tslint -p tsconfig.json -c tslint.json",
        "prettier": "prettier --write *.js **/*.js **/*.ts **/*.tsx",
        "clean": "rm -rf lib",
        "prepack": "npm run clean && npm run build",
        "precommit": "lint-staged",
        "patch": "yarn version --new-version patch && npm publish",
        "minor": "yarn version --new-version minor && npm publish"
    },
    "peerDependencies": {
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "recompose": "*"
    },
    "devDependencies": {
        "@types/react": "16.8.4",
        "@types/react-dom": "16.8.2",
        "@types/recompose": "0.26.1",
        "@types/webpack-env": "1.13.7",
        "prettier": "1.16.4",
        "react": "16.8.1",
        "react-dom": "16.8.1",
        "recompose": "0.30.0",
        "tslint": "5.12.1",
        "tslint-config-prettier": "1.18.0",
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "6.0.0",
        "tslint-plugin-prettier": "2.0.1",
        "tslint-react": "3.6.0",
        "typescript": "3.3.3",
        "vrsource-tslint-rules": "6.0.0"
    },
    "resolutions": {
        "@types/react": "16.8.4",
        "@types/react-dom": "16.8.2"
    }
}
